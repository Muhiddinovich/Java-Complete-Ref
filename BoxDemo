class Box{
    double width; //instance variables
    double height;
    double depth;
}
class Main {
    public static void main(String[] args) {
        Box mybox = new Box(); //instance of the class or object
        double volume;
        
        mybox.width=10;
        mybox.height=20;
        mybox.depth=15;
        
        volume=mybox.width*mybox.height*mybox.depth;
        System.out.println(volume);
    }
}
