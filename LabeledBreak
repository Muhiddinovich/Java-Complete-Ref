class Main {
    public static void main(String[] args) {
        boolean t =true;
        first:{
            second:{
                third:{
                    System.out.println("This will execute");
                    if(t) break second;
                    System.out.println("This won't execute");
                }
                System.out.println("This won't execute");
            }
            System.out.println("After the second block");
            
        }
    }
}
