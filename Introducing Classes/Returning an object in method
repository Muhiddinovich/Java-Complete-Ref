class Test{
    int a;
    
    Test(int i){
        a=i;
    }
    
    Test increaseByTen(){
        Test temp=new Test(a+10);
        return temp;
    }
}
class Main {
    public static void main(String[] args) {
        Test ob1=new Test(2);
        Test ob2;
        ob2=ob1.increaseByTen();
        System.out.println(ob2.a);
        ob2=ob2.increaseByTen();
        System.out.println(ob2.a);
    }
}
