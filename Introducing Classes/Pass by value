class Test{
    //Primitive types are passed by value
    void meth(int i, int j){
        i*=2;
        j/=2;
    }
}
class Main {
    public static void main(String[] args) {
        Test ob=new Test();
        int a=15, b=20;
        System.out.println("Before: "+a+" "+b);
        ob.meth(a,b);
        System.out.println("After: "+a+" "+b);
    }
}
